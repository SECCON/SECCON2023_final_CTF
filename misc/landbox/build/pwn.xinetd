service pwn
{
  disable     = no
  socket_type = stream
  protocol    = tcp
  wait        = no
  user        = ubuntu
  group       = docker
  type        = UNLISTED
  bind        = 0.0.0.0
  port        = 9999
  server      = /home/ubuntu/landbox/build/02_start.sh
  per_source  = 2
  rlimit_as   = UNLIMITED
}
